<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:fitsSystemWindows="true">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/greywhite">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/shape_profile2"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/medbooks_40dp"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@+id/profile_home_avatar"
                        android:layout_width="@dimen/medbooks_70dp"
                        android:layout_height="@dimen/medbooks_70dp"
                        android:layout_gravity="center"
                        android:src="@drawable/no_user" />
                    <TextView
                        android:id="@+id/profile_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/medbooks_5dp"
                        android:textStyle="bold"
                        android:textColor="@color/white"
                        android:text="Nama"
                        android:textSize="@dimen/medbooks_15dp"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="@dimen/medbooks_5dp"
                android:layout_marginBottom="@dimen/medbooks_10dp"
                android:paddingLeft="@dimen/medbooks_15dp"
                android:paddingRight="@dimen/medbooks_15dp">
                <TextView
                    android:id="@+id/biodata"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:gravity="left"
                    android:textStyle="bold"
                    android:background="@color/google_blue"
                    android:drawableRight="@drawable/arrow_down"
                    android:textColor="@color/greywhite"
                    android:paddingBottom="@dimen/medbooks_10dp"
                    android:paddingLeft="@dimen/medbooks_20dp"
                    android:paddingRight="@dimen/medbooks_20dp"
                    android:paddingTop="@dimen/medbooks_10dp"
                    android:text="Biodata"
                    android:textSize="@dimen/medbooks_15dp"
                    android:drawablePadding="@dimen/medbooks_20dp"/>
                <net.cachapa.expandablelayout.ExpandableLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/exp_biodata"
                    app:el_expanded="true"
                    app:el_duration="800">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:padding="@dimen/medbooks_15dp"
                        android:background="@color/greywhite2"
                        android:layout_gravity="center">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/medbooks_3dp"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="2.2"
                                    android:text="NIK"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/medbooks_13dp" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="3"
                                    android:gravity="center"
                                    android:textColor="@color/black"
                                    android:text=":"
                                    android:textSize="@dimen/medbooks_13dp"/>
                                <TextView
                                    android:id="@+id/nik"
                                    android:layout_width="match_parent"
                                    android:layout_weight="1.2"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black"
                                    android:textStyle="bold"
                                    android:text="-"
                                    android:textSize="@dimen/medbooks_13dp"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/medbooks_3dp"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="2.2"
                                    android:text="Nama"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/medbooks_13dp" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="3"
                                    android:gravity="center"
                                    android:textColor="@color/black"
                                    android:text=":"
                                    android:textSize="@dimen/medbooks_13dp"/>
                                <TextView
                                    android:id="@+id/name"
                                    android:layout_width="match_parent"
                                    android:layout_weight="1.2"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black"
                                    android:textStyle="bold"
                                    android:text="-"
                                    android:textSize="@dimen/medbooks_13dp"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/medbooks_3dp"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="2.2"
                                    android:text="Alamat"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/medbooks_13dp" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="3"
                                    android:gravity="center"
                                    android:textColor="@color/black"
                                    android:text=":"
                                    android:textSize="@dimen/medbooks_13dp"/>
                                <TextView
                                    android:id="@+id/alamat"
                                    android:layout_width="match_parent"
                                    android:layout_weight="1.2"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black"
                                    android:textStyle="bold"
                                    android:text="-"
                                    android:textSize="@dimen/medbooks_13dp"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/medbooks_3dp"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="2.2"
                                    android:text="TTL"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/medbooks_13dp" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="3"
                                    android:gravity="center"
                                    android:textColor="@color/black"
                                    android:text=":"
                                    android:textSize="@dimen/medbooks_13dp"/>
                                <TextView
                                    android:id="@+id/ttl"
                                    android:layout_width="match_parent"
                                    android:layout_weight="1.2"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black"
                                    android:textStyle="bold"
                                    android:text="-"
                                    android:textSize="@dimen/medbooks_13dp"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/medbooks_3dp"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="2.2"
                                    android:text="Status hubungan"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/medbooks_13dp" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="3"
                                    android:gravity="center"
                                    android:textColor="@color/black"
                                    android:text=":"
                                    android:textSize="@dimen/medbooks_13dp"/>
                                <TextView
                                    android:id="@+id/status"
                                    android:layout_width="match_parent"
                                    android:layout_weight="1.2"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black"
                                    android:textStyle="bold"
                                    android:text="-"
                                    android:textSize="@dimen/medbooks_13dp"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/medbooks_3dp"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="2.2"
                                    android:text="Kewarganegaraan"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/medbooks_13dp" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="3"
                                    android:gravity="center"
                                    android:textColor="@color/black"
                                    android:text=":"
                                    android:textSize="@dimen/medbooks_13dp"/>
                                <TextView
                                    android:id="@+id/kwg"
                                    android:layout_width="match_parent"
                                    android:layout_weight="1.2"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black"
                                    android:textStyle="bold"
                                    android:text="-"
                                    android:textSize="@dimen/medbooks_13dp"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </net.cachapa.expandablelayout.ExpandableLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginBottom="@dimen/medbooks_10dp"
                android:paddingLeft="@dimen/medbooks_15dp"
                android:paddingRight="@dimen/medbooks_15dp">
                <TextView
                    android:id="@+id/pendidikan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:gravity="left"
                    android:textStyle="bold"
                    android:background="@color/google_blue"
                    android:drawableRight="@drawable/arrow_down"
                    android:textColor="@color/greywhite"
                    android:paddingBottom="@dimen/medbooks_10dp"
                    android:paddingLeft="@dimen/medbooks_20dp"
                    android:paddingRight="@dimen/medbooks_20dp"
                    android:paddingTop="@dimen/medbooks_10dp"
                    android:text="Pendidikan"
                    android:textSize="@dimen/medbooks_15dp"
                    android:drawablePadding="@dimen/medbooks_20dp"/>
                <net.cachapa.expandablelayout.ExpandableLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:el_expanded="false"
                    android:id="@+id/exp_pendidikan"
                    app:el_duration="800">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:padding="@dimen/medbooks_15dp"
                        android:background="@color/greywhite2"
                        android:layout_gravity="center">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/medbooks_3dp"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="2.2"
                                    android:text="Pendidikan"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/medbooks_13dp" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="3"
                                    android:gravity="center"
                                    android:textColor="@color/black"
                                    android:text=":"
                                    android:textSize="@dimen/medbooks_13dp"/>
                                <TextView
                                    android:id="@+id/pendi"
                                    android:layout_width="match_parent"
                                    android:layout_weight="1.2"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black"
                                    android:textStyle="bold"
                                    android:text="-"
                                    android:textSize="@dimen/medbooks_13dp"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </net.cachapa.expandablelayout.ExpandableLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginBottom="@dimen/medbooks_10dp"
                android:paddingLeft="@dimen/medbooks_15dp"
                android:paddingRight="@dimen/medbooks_15dp">
                <TextView
                    android:id="@+id/pekerjaan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:gravity="left"
                    android:textStyle="bold"
                    android:background="@color/google_blue"
                    android:drawableRight="@drawable/arrow_down"
                    android:textColor="@color/greywhite"
                    android:paddingBottom="@dimen/medbooks_10dp"
                    android:paddingLeft="@dimen/medbooks_20dp"
                    android:paddingRight="@dimen/medbooks_20dp"
                    android:paddingTop="@dimen/medbooks_10dp"
                    android:text="Pekerjaan"
                    android:textSize="@dimen/medbooks_15dp"
                    android:drawablePadding="@dimen/medbooks_20dp"/>
                <net.cachapa.expandablelayout.ExpandableLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:el_expanded="false"
                    android:id="@+id/exp_pekerjaan"
                    app:el_duration="800">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:padding="@dimen/medbooks_15dp"
                        android:background="@color/greywhite2"
                        android:layout_gravity="center">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/medbooks_3dp"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="2.2"
                                    android:text="Pekerjaan"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/medbooks_13dp" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="3"
                                    android:gravity="center"
                                    android:textColor="@color/black"
                                    android:text=":"
                                    android:textSize="@dimen/medbooks_13dp"/>
                                <TextView
                                    android:id="@+id/kerja"
                                    android:layout_width="match_parent"
                                    android:layout_weight="1.2"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black"
                                    android:textStyle="bold"
                                    android:text="-"
                                    android:textSize="@dimen/medbooks_13dp"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </net.cachapa.expandablelayout.ExpandableLayout>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
</FrameLayout>
